#pacotes

require(bbmle)
require(glmulti)
require(vegan)
require(caret)
require(dplyr)


#dados
dados=read.table(pipe("pbpaste"), sep="\t", header=T,row.names=1);dados
dados.std <- dados %>% mutate_at(c("Habitat.Diversity", "Distance.Largest.Patch", "Distance.Nearest.Patch", "Area"), ~(scale(.) %>% as.vector))


#multimodel inference  and variable importance
mod=glmulti(IVI~.,data=dados.std,crit=bic,method="h",confsetsize = 130 ,level=1)
coeficientes=coef.glmulti(mod, icmethod="Burnham");coeficientes
  summary.glmulti(mod,2)
weightable(mod)

#best models
top <- weightable(mod)
top <- top[top$aicc <= min(top$aicc) + 2,]

 



